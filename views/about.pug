extends layout

block content
  // contact cards
  .row
    .col-lg-12.text-center
      h3
        i.fa.fa-address-book(aria-hidden='true')
        |  Who We Are
      hr
  .col-lg-4
    .card.contact-card
      img.contact-pic(src='User1.jpg', alt='Omar Shabaro')
      h1 Omar Shabaro
      p.contact-title CEO & Co-Founder
      p Najah University
      .spacing
        a.contact-achor(href='#')
          i.fa.fa-dribbble
        a.contact-achor(href='#')
          i.fa.fa-twitter
        a.contact-achor(href='#')
          i.fa.fa-linkedin
        a.contact-achor(href='#')
          i.fa.fa-facebook
      p
        button.contact-btn Contact
  .col-lg-4
    .card.contact-card
      img.contact-pic(src='User2.jpg', alt='Ahmad Khalaf')
      h1 Ahmad Khalaf
      p.contact-title CTO & Co-Founder
      p Najah University
      .spacing
        a.contact-achor(href='#')
          i.fa.fa-dribbble
        a.contact-achor(href='#')
          i.fa.fa-twitter
        a.contact-achor(href='#')
          i.fa.fa-linkedin
